# EditorConfig IS awesome! http://EditorConfig.org

root = true

# Default to 4 spaces. This is good for lots of languages
[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
insert_final_newline = true

# Set appropriate values for Clojure/Clojurescript and JSON
[*.{clj,cljs,json}]
indent_style = space
indent_size = 4
